{
  "name": "horizon_couture-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build",
    "husky-install": "husky install"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-alice-carousel": "^2.5.1",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "sass": "^1.43.3"
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "eslint-config-next": "11.1.2",
    "prettier": "2.4.1"
  }
}
